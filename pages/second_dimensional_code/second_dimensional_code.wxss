/* pages/second_dimensional_code/second_dimensional_code.wxss */
.container{/*设置view框高等于整个屏幕*/
  width:100%;
  height:100%;
}
.images{
width:100%;
height: 300rpx;
display: flex;
flex-direction: row;
}
.img{
width:50%;
height:100%;
display: flex;/*水平垂直居中要在这个条件下才能起到作用*/
align-items: center;
justify-content: center;
}
.concrete_img{
   height:200rpx;
  width:300rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border: 1rpx black solid;
  border-radius: 10%;
 
}
.concrete_img image{
  height:160rpx;
  width:160rpx;
}
.vip{
  position: relative;
  height:80rpx;
  width:80rpx;
  border-radius:20%;
  right: 10rpx;
  left:225rpx;
  top: 80rpx;
}